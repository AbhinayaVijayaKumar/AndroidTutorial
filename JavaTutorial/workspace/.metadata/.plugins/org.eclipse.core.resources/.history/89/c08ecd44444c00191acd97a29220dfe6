
public class prac1 {

	public static void main(String[] args) {
		//char letter1 = 'C';
		//char letter2 = ++letter1;
		//System.out.println("The value is " + letter2);
		
		int a = 5;
		int y2 = -a++;
		System.out.println("The value is " + y2);
		int y3 = -++a;
		System.out.println("The value is " + y3);
		int y4 = +--a;
		System.out.println("The value is " + y4);

	}

}
